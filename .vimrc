execute pathogen#infect()
syntax on
filetype plugin indent on
syntax enable
set shiftwidth=4
set softtabstop=4
set cursorline
set ignorecase
set smartcase
set hlsearch
set noerrorbells
set ai
set si
set wrap
set showcmd
set lazyredraw
set showmatch
inoremap jk <esc>
set number
colorscheme gruvbox
set bg=dark
